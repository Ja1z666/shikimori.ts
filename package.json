{
  "name": "shikimori.ts",
  "description": "JavaScript & TypeScript wrapper for shikimori.one",
  "version": "1.0.1",
  "main": "lib/shikimori.js",
  "types": "lib/shikimori.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ja1z666/shikimori.ts.git"
  },
  "keywords": [
    "shikimori",
    "shiki",
    "shiki-api",
    "shikimori-api",
    "wrapper"
  ],
  "author": "Ja1z666",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ja1z666/shikimori.ts/issues"
  },
  "homepage": "https://github.com/Ja1z666/shikimori.ts#readme",
  "dependencies": {
    "@types/node": "^17.0.8",
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
